.my-search-container {
  display: flex;
  width: 100%;
}
.my-search-container .my-input-wp {
  display: flex;
  align-items: center;
  flex-direction: row;
  justify-content: flex-start;
  width: 95%;
  height: 96rpx;
  padding: 15px;
  margin: 0 auto;
  border: 1px solid #cbcbcb;
  border-radius: 15px;
  box-sizing: border-box;
}
.my-search-container .my-search-icon {
  width: 40rpx;
  height: 40rpx;
}
.my-search-container .place-holder {
  font: normal 32rpx/40rpx 'Microsoft YaHei';
  color: #cbcbcb;
}
.my-search-container .my-search-bar {
  width: 100%;
}

